interface AdminRole {
	void addNewProduct();
	void deleteProduct();
	void viewAllProducts();
	void search() ;
}
interface CustomerRole {
	void viewAllProducts();
	void search() ;
}
class Product implements AdminRole, CustomerRole{
	public void addNewProduct() {
		System.out.println("Added..");
	}
	public void deleteProduct() {
		System.out.println("Deleted...");
	}
	public void viewAllProducts() {
		System.out.println("Listed...");
	}
	public void search() {
		System.out.println("Searching...");
	}
}
public class Test {

	public static void main(String[] args) {
		//To Admin
		AdminRole service = new Product();//Loose Coupling
		service.addNewProduct();
		service.viewAllProducts();
		service.deleteProduct();
		service.search();
		
		//To Customer
		CustomerRole customer = new Product();
		customer.search();
		customer.viewAllProducts();

	}

}
