class A {
	int i;
	int k;
	void display(){
		System.out.println(" A ");
	}
}
class B extends A {
	int j;
	void print() {
		System.out.println(" B ");
	}
}

public class Test {

	public static void main(String[] args) {
		A a = new A();
		a.i = 100;
		System.out.println(a.i);
		a.display();
		
		B b = new B();
		b.i = 500;
		b.j = 1000;
		b.k=200;
		System.out.println(b.i);
		System.out.println(b.j);
		b.display();
		b.print();
		
	}

}




