class A {
	public void display() {
		System.out.println("Class - A");
	}
}
class B extends A {
	@Override
	public void display() {
		System.out.println("Class - B");
	}
}
class C extends A {
	@Override
	public void display() {
		System.out.println("Class - C");
	}
}
