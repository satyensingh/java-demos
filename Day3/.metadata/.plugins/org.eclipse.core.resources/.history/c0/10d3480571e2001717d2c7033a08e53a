
class A {
	public A() {
		super();//Calling the base class ctor.
		System.out.println("A Ctor....");
	}
	void display() {
		
	}
}
class B extends A {
	public B() {
		super();//Calling the base class ctor.
		System.out.println("B Ctor...");
	}
}

public class Test {

	public static void main(String[] args) {
		
		A a = new A();
		B b = new B();
		
	}

}




