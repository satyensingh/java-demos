class Abc {
	final int num2;//This is an instance member which is final
	static final int num = 100;//This is a class member and final at the same time
	public Abc() {
		num2 = 200;
	}
	public void print() {
		System.out.println(num);
		System.out.println(num2);
	}
}

public class Test {

