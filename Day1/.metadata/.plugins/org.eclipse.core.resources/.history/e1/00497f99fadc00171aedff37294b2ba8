
public class Test {

	public static void main(String[] args) {
		boolean found=false;
		
		String names[] = {"Satyen","Vikram","Pooja","Vibhu","Dinesh", "Pooh", "John", "Matt"};
		
		String nameToSearch = "vikram";
		
		for (String name : names) {
			if(name.equalsIgnoreCase(nameToSearch)) {
				System.out.println("Yes, "+nameToSearch+" exist in the list.");
				found = true;
				break;
			}
		}
		if(!found) {
			System.out.println("Name "+nameToSearch+" does not exist in the list");
		}
		System.out.println("Operation Successfull...");
	}

}
