		SavingsAccount account2 = new SavingsAccount("Abc", true);
		SavingsAccount account3 = new SavingsAccount("Abc", true);
		SavingsAccount account4 = new SavingsAccount("Abc", true);
		SavingsAccount account5 = new SavingsAccount("Abc", true);
		
		
		//Static methods are invoked using class name
